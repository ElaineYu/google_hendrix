    </script>

    <script type="text/javascript">
       function initialize() {
        var mapOptions = {
          center: new google.maps.LatLng(25.002394,54.992554),
          zoom: 11,
          mapTypeId: google.maps.MapTypeId.SATELLITE
        };
        var mapProp2 = {
          center: new google.maps.LatLng(25.002394,54.992554),
